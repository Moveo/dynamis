{
  "name": "dynamis",
  "version": "0.1.1",
  "description": "Node.JS cache API for various persistence layers",
  "main": "index.js",
  "scripts": {
    "test": "NODE_ENV=test mocha $(find test -name '*.test.js')",
    "coverage": "NODE_ENV=test istanbul cover ./node_modules/.bin/_mocha $(find test -name '*.test.js') --report lcovonly -- -R spec && cat ./coverage/lcov.info | ./node_modules/coveralls/bin/coveralls.js || true && rm -rf ./coverage"
  },
  "repository": {
    "type": "git",
    "url": "git://github.com/swaagie/dynamis.git"
  },
  "keywords": [
    "Cache",
    "singular",
    "API",
    "persistence",
    "couchDB",
    "redis",
    "levelDB",
    "memory",
    "node",
    "js"
  ],
  "author": "Martijn Swaagman",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/swaagie/dynamis/issues"
  },
  "homepage": "https://github.com/swaagie/dynamis",
  "dependencies": {
    "async": "~1.4.0",
    "debug": "~2.2.0",
    "eventemitter3": "~1.1.1",
    "fusing": "~1.0.0"
  },
  "devDependencies": {
    "chai": "~3.2.0",
    "coveralls": "~2.11.0",
    "cradle": "~0.6.7",
    "istanbul": "~0.3.17",
    "memcached": "~2.1.0",
    "mocha": "~2.2.5",
    "mocha-lcov-reporter": "~0.0.1",
    "pre-commit": "~1.1.1",
    "redis": "~0.10.1",
    "sinon": "~1.15.4",
    "sinon-chai": "~2.8.0"
  }
}
