{
  "name": "dynamis",
  "version": "0.1.1",
  "description": "Node.JS cache API for various persistence layers",
  "main": "index.js",
  "scripts": {
    "test": "NODE_ENV=test ./node_modules/.bin/mocha $(find test -name '*.test.js')",
    "coverage": "NODE_ENV=test ./node_modules/.bin/istanbul cover ./node_modules/.bin/_mocha $(find test -name '*.test.js') --report lcovonly -- -R spec && cat ./coverage/lcov.info | ./node_modules/coveralls/bin/coveralls.js || true && rm -rf ./coverage"
  },
  "repository": {
    "type": "git",
    "url": "git://github.com/Moveo/dynamis.git"
  },
  "keywords": [
    "Cache",
    "singular",
    "API",
    "persistence",
    "couchDB",
    "redis",
    "levelDB",
    "memory",
    "node",
    "js"
  ],
  "author": "Martijn Swaagman",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/Moveo/dynamis/issues"
  },
  "homepage": "https://github.com/Moveo/dynamis",
  "dependencies": {
    "async": "~0.2.10",
    "debug": "~0.7.4",
    "eventemitter3": "~0.1.1",
    "fusing": "~0.1.0"
  },
  "devDependencies": {
    "chai": "~1.9.0",
    "coveralls": "~2.8.0",
    "cradle": "~0.6.7",
    "istanbul": "~0.2.6",
    "memcached": "~0.2.8",
    "mocha": "~1.18.0",
    "mocha-lcov-reporter": "~0.0.1",
    "pre-commit": "~0.0.4",
    "redis": "~0.10.1",
    "sinon": "~1.9.0",
    "sinon-chai": "~2.5.0"
  }
}
